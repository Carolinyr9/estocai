				{
					"name": "✅ Login - Sucesso (Armazena Token)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Analisa a resposta JSON",
									"var jsonData = pm.response.json();",
									"",
									"// Verifica se a resposta tem o campo 'token'",
									"if (jsonData && jsonData.token) {",
									"    // Armazena o token na variável de coleção 'token'",
									"    pm.collectionVariables.set(\"token\", jsonData.token);",
									"    console.log(\"Token de Acesso Salvo: \" + jsonData.token);",
									"    pm.test(\"Status code is 200\", function () {",
									"        pm.response.to.have.status(200);",
									"    });",
									"    pm.test(\"Token is present and saved\", function () {",
									"        pm.expect(jsonData.token).to.be.a('string');",
									"    });",
									"} else {",
									"    pm.test(\"Token is NOT present in response body\", false);",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
